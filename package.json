{
  "name": "webpack-iconfont-plugin",
  "version": "0.0.1",
  "description": "Webpack plugin to create iconfont from svgs",
  "license": "MIT",
  "author": "Jens Fischer <jens@weareprofound.com>",
  "repository": {
    "type": "https",
    "url": "https://github.com/artofrawr/webpack-iconfont-plugin"
  },
  "scripts": {
    "test": "jest"
  },
  "keywords": [
    "icons",
    "font",
    "iconfont",
    "svg",
    "scss",
    "webpack",
    "plugin"
  ],
  "bugs": {
    "url": "https://github.com/artofrawr/webpack-iconfont-plugin/issues"
  },
  "babel": {
    "presets": [
      "es2015",
      "latest"
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/__tests__/assets/",
      "<rootDir>/__tests__/build/",
      "<rootDir>/__tests__/config/"
    ]
  },
  "main": "index.js",
  "dependencies": {
    "async-throttle": "^1.1.0",
    "fs-extra": "^3.0.0",
    "glob-parent": "^3.1.0",
    "globby": "^6.1.0",
    "hasha": "^3.0.0",
    "nodeify": "^1.0.0",
    "nunjucks": "^3.0.1",
    "svg2ttf": "^4.0.3",
    "svgicons2svgfont": "^5.0.1",
    "ttf2eot": "^2.0.0",
    "ttf2woff": "^2.0.1",
    "ttf2woff2": "^2.0.3",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "del": "^3.0.0",
    "jest": "^20.0.4",
    "webpack": "^3.0.0"
  }
}
